{% if
  settings.sm_facebook_link != blank or
  settings.sm_twitter_link != blank or
  settings.sm_youtube_link != blank or
  settings.sm_flickr_link != blank or
  settings.sm_pinterest_link != blank or
  settings.sm_instagram_link != blank %}

<ul id="sm-icons" class="clearfix list-soc">

  {% if settings.sm_facebook_link != blank %}
    <li id="sm-facebook"><a href="{{settings.sm_facebook_link}}" target="_blank"><i class="fa fa-facebook"></i> Facebook</a></li>
  {% endif %}

  {% if settings.sm_twitter_link != blank %}
    <li id="sm-twitter"><a href="{{settings.sm_twitter_link}}" target="_blank"><i class="fa fa-twitter"></i></a></li>
  {% endif %}

  {% if settings.sm_instagram_link != blank %}
    <li id="sm-instagram"><a href="{{settings.sm_instagram_link}}" target="_blank"><i class="fa fa-instagram"></i> Instagram</a></li>
  {% endif %}

  {% if settings.sm_tumblr_link != blank %}
    <li id="sm-tumblr"><a href="{{settings.sm_tumblr_link}}" target="_blank"><i class="fa fa-tumblr"></i></a></li>
  {% endif %}

  {% if settings.sm_pinterest_link != blank %}
    <li id="sm-pinterest"><a href="{{settings.sm_pinterest_link}}" target="_blank"><i class="fa fa-pinterest"></i></a></li>
  {% endif %}

  {% if settings.sm_youtube_link != blank %}
    <li id="sm-youtube"><a href="{{settings.sm_youtube_link}}" target="_blank"><i class="fa fa-youtube"></i></a></li>
  {% endif %}

  {% if settings.sm_vimeo_link != blank %}
    <li id="sm-vimeo"><a href="{{settings.sm_vimeo_link}}" target="_blank"><i class="fa fa-vimeo"></i></a></li>
  {% endif %}

  {% if settings.sm_flickr_link != blank %}
    <li id="sm-flickr"><a href="{{settings.sm_flickr_link}}" target="_blank"><i class="fa fa-flickr"></i></a></li>
  {% endif %}

  {% if settings.sm_snapchat_link != blank %}
    <li id="sm-snapchat"><a href="{{settings.sm_snapchat_link}}" target="_blank"><i class="fa fa-snapchat"></i></a></li>
  {% endif %}

  {% if settings.sm_houzz_link != blank %}
    <li id="sm-houzz"><a href="{{settings.sm_houzz_link}}" target="_blank"><i class="fa fa-houzz"></i></a></li>
  {% endif %}

  {% if settings.sm_linkedin_link != blank %}
    <li id="sm-linkedin"><a href="{{settings.sm_linkedin_link}}" target="_blank"><i class="fa fa-linkedin"></i></a></li>
  {% endif %}

  {% if settings.sm_google_plus_link != blank %}
    <li id="sm-google_plus"><a href="{{settings.sm_google_plus_link}}" target="_blank"><i class="fa fa-google-plus"></i></a></li>
  {% endif %}

</ul>

{% endif %}
